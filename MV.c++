void accionHambre() {
  // Mostrar la imagen "Need" primero
  drawBitmap(need_bitmap_array[0], 128, 64);

  // Esperar hasta que se presione el botón
  Serial.println("Esperando a que se presione el botón para mostrar la animación de hambre...");
  while (digitalRead(BUTTON_PIN) == HIGH) {
    delay(10);
    client.loop();  // Procesar el cliente MQTT durante el bucle
  }

  // Mostrar la animación de "hambre"
  Serial.println("Botón presionado, mostrando animación de hambre.");
  for (int i = 0; i < hambre_bitmap_len; i++) {
    drawBitmap(hambre_bitmap_array[i], 128, 64);
    unsigned long startTime = millis();
    while (millis() - startTime < 500) {
      delay(10);
      client.loop();  // Procesar el cliente MQTT durante la animación
    }
  }

  // **Enviar mensaje MQTT indicando que 'hambre' ahora es 1**
  if (!client.connected()) {
    Serial.println("MQTT no conectado, intentando reconectar...");
    reconnect();
  }

  StaticJsonDocument<200> doc;
  doc["hambre"] = 1;
  String mensaje;
  serializeJson(doc, mensaje);
  boolean result = client.publish("toMQTT", mensaje.c_str());
  if (result) {
    Serial.println("Mensaje enviado: " + mensaje);
  } else {
    Serial.println("Error al enviar mensaje MQTT");
  }

  // Procesar el cliente MQTT para asegurar que el mensaje se envíe
  client.loop();

  mostrarImagenFinal();
}


