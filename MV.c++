void reconnect() {
  while (!client.connected()) {
    Serial.print("Intentando conexión MQTT...");
    String clientId = "ESP32Client-";
    clientId += String(random(0xffff), HEX);
    if (client.connect(clientId.c_str())) {
      Serial.println("conectado");

      // Suscribirse al tópico aquí
      client.subscribe("MQTTestado");
      Serial.println("Suscrito al tópico MQTTestado");
    } else {
      Serial.print("falló, rc=");
      Serial.print(client.state());
      Serial.println(" intentando nuevamente en 5 segundos");
      delay(5000);
    }
  }
}

void setup() {
  Serial.begin(115200);
  analogSetPinAttenuation(pinLDR, ADC_11db);
  setup_wifi();
  client.setServer(mqtt_server, 1883);
  client.setCallback(callback);
  dht.begin();

  // Inicializar pantalla OLED
  display.begin(SH1106_I2C_ADDRESS, 0x3C);
  display.clearDisplay();
  display.display();

  // Configurar el botón
  pinMode(BUTTON_PIN, INPUT_PULLUP);  
  pinMode(boton_Doce, INPUT_PULLUP);
  pinMode(ventilador, OUTPUT);
  pinMode(buzzerPin, OUTPUT);

}

void loop() {
  // Control de la animación inicial
  if (!pulsadorPresionado) {
    // Verificamos si el pulsador ha sido presionado
    if (digitalRead(boton_Doce) == LOW) {
      // Si se presiona el botón, mostramos la imagen final
      pulsadorPresionado = true;
      Serial.println("Pulsador presionado. Mostrando imagen final.");
      mostrarImagenFinal();
    } else {
      // Si el pulsador no ha sido presionado, mostramos la animación del huevo
      mostrarAnimacionHuevo();
    }
    delay(100);
  } else {
    // Una vez que se presionó el botón, continúa con el funcionamiento normal
    if (!client.connected()) {
      reconnect();
    }
    client.loop();

  unsigned long currentMillis = millis();

  // Manejar el estado de mostrar alerta y animación
  if (estado2Activo) {
    if (mostrandoAlerta) {

      // Esperar la duración de la alerta antes de iniciar la animación
      if (currentMillis - tiempoInicioAlerta >= duracionAlerta) {
        mostrandoAlerta = false;  // Terminar de mostrar la alerta
        estado2FrameIndex = 0;    // Reiniciar índice de animación
        previousEstado2Millis = currentMillis;  // Reiniciar temporizador de animación
      }
    } else {
      // Manejar la animación y el buzzer
      // Actualizar la animación
      if (currentMillis - previousEstado2Millis >= estado2FrameInterval) {
        previousEstado2Millis = currentMillis;

        // Mostrar el siguiente frame de la animación "Fuego"
        drawBitmap(fuego_bitmap_array[estado2FrameIndex], 128, 64);

        estado2FrameIndex++;
        if (estado2FrameIndex >= fuego_bitmap_len) {
          estado2FrameIndex = 0;  // Reiniciar la animación
        }
      }

      // Manejar el buzzer
      if (currentMillis - previousBuzzerMillis >= buzzerInterval) {
        previousBuzzerMillis = currentMillis;

        if (buzzerEncendido) {
          noTone(buzzerPin);
          buzzerEncendido = false;
        } else {
          tone(buzzerPin, 1000);  // Frecuencia de 1000 Hz
          buzzerEncendido = true;
        }
      }
    }
    // Asegurarse de que el ventilador esté encendido
    digitalWrite(ventilador, HIGH);
  } else {
    // Asegurarse de que el buzzer y el ventilador estén apagados
    noTone(buzzerPin);
    buzzerEncendido = false;
    digitalWrite(ventilador, LOW);
  }
